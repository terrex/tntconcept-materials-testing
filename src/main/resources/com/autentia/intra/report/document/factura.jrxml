	<queryString><![CDATA[SELECT DISTINCT bb.amount as PrecioParcial,bb.concept as Concepto, bb.iva as Iva, bb.units as Unidades,pv.name as Provincia,o.name as OrgName,o.cif,o.phone,o.street,o.number as OrgNumber,
	o.locator,o.postalCode,o.city,b.id,b.creationDate,b.expiration,b.paymentMode,
	b.number as BillNumber,b.name as BillName,b.observations,b.orderNumber,
	c.name as ContactName 
FROM Bill b left join Contact c on b.contactId=c.id,

(
/*costes directos a la factura*/
(select amount, concept, iva, units from BillBreakDown where billId=$P{id})

/*informes sin familia de pauta de albaranes*/
union(select coalesce(coste_project_sin_costes_ni_tareas_sin_iva(p.id),0),
concat('Informe ',p.referenciaLaboratorio,' (',aa.name,')'), 16, 1
from Project p
join Albaran aa on p.albaranId=aa.id
where aa.billId=$P{id}
and
(
    (informe_tiene_pauta(p.id) and not informe_tiene_familia(p.id))
    or
    (not informe_tiene_pauta(p.id))
)
group by 1, p.referenciaLaboratorio
)

/*informes con familia de pauta de albaranes*/
union(select coalesce(coste_project_sin_costes_ni_tareas_sin_iva(p.id),0),
concat('Informes de la familia ',pa.familia,' (',aa.name,')'), 16, count(p.id)
from Project p
join Albaran aa on p.albaranId=aa.id
join Pauta pa on p.especificacion=pa.name
where coalesce(pa.familia,'')!=''
and aa.billId=$P{id}
group by 1, pa.familia, aa.name)

/*tareas de presupuesto*/
union(select _or.costPerHour, concat(_or.name,' (',_o.number,')'), _o.iva, _or.expectedHours from OfferRole _or join Offer _o on (_or.offerId=_o.id) where _o.billId=$P{id})

/*costes de presupuesto*/
union(select _oc.cost, concat(_oc.name,' (',_o.number, ')'), _o.iva, _oc.billable from OfferCost _oc join Offer _o on (_oc.offerId=_o.id) where _o.billId=$P{id} and _oc.billable = 1)

) as bb,
Project p, Organization o, Province pv
WHERE o.id=b.clientId and pv.id = o.provinceId 
	 and b.id=$P{id};]]></queryString>

