	<queryString><![CDATA[select o.columnaRefCli,
o.name as cliente,
a.name as numero,
a.fecha,
p.referenciaCliente as albaran,
p.referenciaLaboratorio,
a.devuelto,
(
case when p.costeAFacturar is not null and p.costeAFacturar > 0 then p.costeAFacturar
else ((select ifnull(sum(px.costPerHour*px.expectedHours),0) from ProjectRole px where px.projectId=p.id)
+(select ifnull(sum(px.cost),0) from ProjectEnsayo px where px.projectId=p.id)
+(select ifnull(sum(px.cost*px.billable),0) from ProjectCost px where px.projectId=p.id))
end
) as cost
from Albaran a
join Project p on(a.id=p.albaranId)
join Organization o on (a.client=o.id)
where a.id = $P{id}]]></queryString>

