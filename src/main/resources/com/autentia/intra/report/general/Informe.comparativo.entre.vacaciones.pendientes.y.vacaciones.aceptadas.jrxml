    <queryString><![CDATA[SELECT
	aceptadas.userName as 'userName',
	COALESCE(aceptadas.dias,0) as 'aceptados',
	COALESCE(imputadas.dias,0) as 'imputados',
	(COALESCE(aceptadas.dias,0)-COALESCE(imputadas.dias,0)) as diferencia

FROM ( SELECT 
	u.name AS 'userName', 
	sum(DATEDIFF(finalDate,beginDate)+1) as 'dias'
       FROM 
	User u, 
	RequestHoliday r
       WHERE 
	r.userId=u.id and 
	r.state='ACCEPT' and 
	r.beginDate between $P{Fecha inicio} and $P{Fecha fin} 
       GROUP BY u.id) as aceptadas 

LEFT JOIN

     ( SELECT 
	u.name AS 'userName',
	count(*) AS 'dias'
       FROM 
	Project p, 
	Activity a, 
	ProjectRole r, 
	User u 
       WHERE 
	p.name LIKE '%Vacaciones' and 
	a.roleId=r.id and 
	p.id=r.projectId and 
	a.userId=u.id and 
	a.startDate between $P{Fecha inicio} and $P{Fecha fin}
       GROUP BY u.id ) as imputadas 

ON imputadas.userName = aceptadas.userName
ORDER BY imputadas.userName]]></queryString>

