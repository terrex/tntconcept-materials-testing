    <queryString><![CDATA[SELECT projectName,fact,noFact,hours
FROM (SELECT sum(duration) as 'hours',p.name AS 'projectName',
		 sum(CASE WHEN a.billable=1
   		 then duration end) as 'fact',
		 sum(CASE WHEN a.billable=0
		  then duration end) as 'noFact'
	FROM Activity a,Project p,ProjectRole r 
	WHERE a.roleId=r.id and p.id=r.projectId 
	and a.startDate between $P{Fecha inicio} and $P{Fecha fin}
	GROUP BY p.id 
	ORDER BY hours desc) as tabla;]]></queryString>

