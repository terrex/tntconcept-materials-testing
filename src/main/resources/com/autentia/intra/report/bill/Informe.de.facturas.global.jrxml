	<queryString><![CDATA[SELECT b.id as billId, case b.state when 'PAID' then 'Pagada'
                    when 'EMITTED' then 'Emitida'
                    end as estado,b.number,o.name,b.creationDate, b.expiration AS expiration
	,coste_factura_sin_iva(b.id) as sumaFactura,
(select distinct case when x.parentId is null or x.parentId = 1 then x.name else concat(y.name, ' \\ ', x.name) end from Department x, Department y where (y.id = x.parentId or x.parentId is null) and x.id=$P{Linea de trabajo}) as lineaTrabajo
from Bill b
left join Organization o on b.clientId = o.id
where b.billType='issued'
and b.creationDate between $P{Fecha inicio} and $P{Fecha fin} 
and ($P{Linea de trabajo} is null or b.lineaTrabajoId in (select hijo from Department_hijos where padre = $P{Linea de trabajo}))
group by b.id
order by b.number desc]]></queryString>

