	<queryString><![CDATA[select tabla.mes,tabla.orgName,COALESCE(tabla.sumaEmitidasTotal,0) as sumaE,COALESCE(tablas.sumaPagadasTotal,0) as sumaP,(COALESCE(tabla.sumaEmitidasTotal,0)-COALESCE(tablas.sumaPagadasTotal,0)) as diferencia
from (
SELECT orgName,mes,sum(sumaEmitidas) as sumaEmitidasTotal
FROM (SELECT o.name as 'orgName',quarter(b.creationDate) as mes,COALESCE(sum(bb.amount*bb.units*(1 + (bb.iva/100))),0) as sumaEmitidas 
	from Bill b, Organization o, Project p, BillBreakDown bb 
	where b.projectId = p.id 
	and o.id=$P{Organizacion} 
	and p.organizationId=o.id 
	and bb.billId=b.id and b.billType='issued'	
	and b.creationDate between $P{Fecha inicio} and $P{Fecha fin}
group by b.id) as t group by mes) as tabla
left join
(SELECT mes,sum(sumaPagadas) as sumaPagadasTotal
FROM (SELECT quarter(b.creationDate) as mes,COALESCE(sum(coste_factura_sin_iva(b.id)),0) as sumaPagadas from Bill b, Organization o
	where o.id=$P{Organizacion}
	and b.clientId=$P{Organizacion} 
	and b.state='paid' and b.billType='issued'
	and b.creationDate between $P{Fecha inicio} and $P{Fecha fin}
	group by b.id) as t2 group by mes) as tablas
on tabla.mes = tablas.mes]]></queryString>

