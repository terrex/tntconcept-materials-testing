	<queryString><![CDATA[SELECT b.number,o.name,b.creationDate,b.expiration, COALESCE(sum(coste_factura_sin_iva(b.id)),0) as sumaTotal,(b.creationDate+b.expiration) as nada
from Bill b left join Organization o on o.id = b.clientId
where b.state!='paid' and b.billType='issued'
and DATEDIFF(CURRENT_DATE,creationDate) > b.expiration
group by b.id
order by creationDate desc]]></queryString>

