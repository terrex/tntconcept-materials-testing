    <queryString><![CDATA[select orgName,userName,startDate,duration,description,fact,noFact, projectName from (

SELECT o.name as orgName,u.name as userName,p.name as projectName,
 a.startDate,a.duration,a.description,sum(CASE WHEN a.billable=1
   					       then duration end) as 'fact',
				      sum(CASE WHEN a.billable=0
				      then duration end) as 'noFact' 
FROM Activity a,Organization o,User u,Project p,ProjectRole r, ProjectEnsayo pe
where u.id=a.userId and (r.id=a.roleId or pe.id=a.projectEnsayoId) and p.organizationId=o.id and p.id=r.projectId and p.id=pe.projectId
and p.id=$P{Proyecto} and a.startDate between $P{Fecha inicio} and $P{Fecha fin}
and a.billable=1
group by a.id
order by a.startDate,u.name) as tabla;]]></queryString>

