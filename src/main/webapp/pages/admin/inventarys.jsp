<%@page language="java" contentType="text/html; charset=UTF-8" %>

<%@include file="/inc/tlibs.jsp" %>

<html>
<head>
    <%@include file="/inc/uiCore.jsp" %>
</head>
<body>

<!-- inventarys.jsp: generated by stajanov code generator -->
<f:loadBundle basename="com.autentia.intra.resources.messages" var="msg"/>
<i:location place="inventarys" msg="${msg}"/>

<f:view>
    <%@include file="/inc/header.jsp" %>
    <h:form id="inventarys">

        <%-- Header --%>
        <i:titleBar msg="${msg}">
            <h:commandLink action="#{inventaryBean.search}">
                <h:graphicImage rendered="#{ inventaryBean.search.searchActive}" title="#{msg.entityActions_filtered}"
                                value="/img/search_applied.gif" styleClass="titleImg"/>
                <h:graphicImage rendered="#{!inventaryBean.search.searchActive}" title="#{msg.entityActions_search}"
                                value="/img/search.gif" styleClass="titleImg"/>
            </h:commandLink>
            <h:commandLink action="#{inventaryBean.reset}">
                <h:graphicImage rendered="#{inventaryBean.search.searchActive}" title="#{msg.entityActions_reset}"
                                value="/img/eraser.png" styleClass="titleImg"/>
            </h:commandLink>
            <h:commandLink action="#{inventaryBean.create}" rendered="#{inventaryBean.createAvailable}">
                <h:graphicImage title="#{msg.entityActions_create}" value="/img/new.gif" styleClass="titleImg"/>
            </h:commandLink>
        </i:titleBar>


        <%-- List of inventarys --%>
        <t:dataTable id="list" var="inventary" value="#{inventaryBean.all}" preserveDataModel="false"
                     cellpadding="0" cellspacing="0" styleClass="listTable"
                     headerClass="listHeaderCell" footerClass="listFooter"
                     rows="#{settingBean.mySettings.listSize}" rowClasses="listRowO,listRowE"
                     columnClasses="listCmdCell,listInventarySerialNumber,listInventaryTrademark,listInventaryModel,listInventaryAssignedTo"
                     sortColumn="#{inventaryBean.sortColumn}" sortAscending="#{inventaryBean.sortAscending}"
                     rowOnMouseOver="this.savedClassName=this.className;this.className='listRowSel';"
                     rowOnMouseOut="this.className=this.savedClassName;">

            <%-- Commands --%>
            <h:column>
                <f:facet name="header">
                    <f:verbatim>-</f:verbatim>
                </f:facet>
                <t:commandLink action="#{inventaryBean.detail}" immediate="true">
                    <f:param name="id" value="#{inventary.id}"/>
                    <h:graphicImage title="#{msg.entityActions_detail}" value="/img/detail.gif" styleClass="cmdImg"/>
                </t:commandLink>
            </h:column>


            <%-- Ignored field: id --%>


            <%-- Ignored field: buyDate --%>


            <%-- Ignored field: renting --%>


            <%-- Ignored field: cost --%>


            <%-- Ignored field: amortizable --%>


            <%-- Field: serialNumber --%>
            <h:column>
                <f:facet name="header">
                    <t:commandSortHeader styleClass="listHeader" columnName="serialNumber">
                        <f:facet name="ascending">
                            <t:graphicImage value="/img/ascending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:facet name="descending">
                            <t:graphicImage value="/img/descending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:verbatim>${msg['inventary.serialNumber']}</f:verbatim>
                    </t:commandSortHeader>
                </f:facet>

                <t:commandLink action="#{inventaryBean.detail}" immediate="true">
                    <f:param name="id" value="#{inventary.id}"/>
                    <%-- String field --%>
                    <h:outputText value="#{inventary.serialNumber}"/>
                </t:commandLink>

            </h:column>


            <%-- Ignored field: provider --%>


            <%-- Field: trademark --%>
            <h:column>
                <f:facet name="header">
                    <t:commandSortHeader styleClass="listHeader" columnName="trademark">
                        <f:facet name="ascending">
                            <t:graphicImage value="/img/ascending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:facet name="descending">
                            <t:graphicImage value="/img/descending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:verbatim>${msg['inventary.trademark']}</f:verbatim>
                    </t:commandSortHeader>
                </f:facet>

                <t:commandLink action="#{inventaryBean.detail}" immediate="true">
                    <f:param name="id" value="#{inventary.id}"/>
                    <%-- String field --%>
                    <h:outputText value="#{inventary.trademark}"/>
                </t:commandLink>

            </h:column>


            <%-- Field: model --%>
            <h:column>
                <f:facet name="header">
                    <t:commandSortHeader styleClass="listHeader" columnName="model">
                        <f:facet name="ascending">
                            <t:graphicImage value="/img/ascending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:facet name="descending">
                            <t:graphicImage value="/img/descending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:verbatim>${msg['inventary.model']}</f:verbatim>
                    </t:commandSortHeader>
                </f:facet>

                <t:commandLink action="#{inventaryBean.detail}" immediate="true">
                    <f:param name="id" value="#{inventary.id}"/>
                    <%-- String field --%>
                    <h:outputText value="#{inventary.model}"/>
                </t:commandLink>

            </h:column>


            <%-- Ignored field: speed --%>


            <%-- Ignored field: storage --%>


            <%-- Ignored field: ram --%>


            <%-- Ignored field: location --%>


            <%-- Ignored field: description --%>


            <%-- Ignored field: type --%>


            <%-- Ignored field: ownerId --%>


            <%-- Ignored field: departmentId --%>


            <%-- Ignored field: insertDate --%>


            <%-- Ignored field: updateDate --%>


            <%-- Field: assignedTo --%>
            <h:column>
                <f:facet name="header">
                    <t:commandSortHeader styleClass="listHeader" columnName="assignedTo.name">
                        <f:facet name="ascending">
                            <t:graphicImage value="/img/ascending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:facet name="descending">
                            <t:graphicImage value="/img/descending-arrow.gif" border="0"/>
                        </f:facet>
                        <f:verbatim>${msg['inventary.assignedTo']}</f:verbatim>
                    </t:commandSortHeader>
                </f:facet>

                <t:commandLink action="#{inventaryBean.detail}" immediate="true">
                    <f:param name="id" value="#{inventary.id}"/>
                    <%-- Referenced field --%>
                    <h:outputText value="#{inventary.assignedTo.name}"/>

                </t:commandLink>

            </h:column>

        </t:dataTable>

        <%-- Paginator control --%>
        <%@include file="/inc/paginator.jsp" %>

    </h:form>
</f:view>

</body>
</html>  		

