<%@page language="java" contentType="text/html; charset=UTF-8" %>

<%@include file="/inc/tlibs.jsp" %>

<html>
<head>
    <%@include file="/inc/uiCore.jsp" %>
</head>
<body>

<!-- searchPublication.jsp: generated by stajanov code generator -->
<f:loadBundle basename="com.autentia.intra.resources.messages" var="msg"/>
<i:location place="searchPublication" msg="${msg}"/>

<f:view>
<%@include file="/inc/header.jsp" %>
<h:form id="search" enctype="multipart/form-data">

<%-- Header --%>
<i:titleBar msg="${msg}">
    <h:commandLink action="#{publicationBean.list}">
        <h:graphicImage title="#{msg.entityActions_run}" value="/img/run.png" styleClass="titleImg"/>
    </h:commandLink>
</i:titleBar>

<%-- Search form --%>
<table class="searchTable" cellpadding="0" cellspacing="0">


    <%-- Ignored field: id --%>


    <%-- Field: name --%>
<tr>
    <td class="searchLabel">${msg['publication.name']}:</td>
    <td class="searchFieldCell">


        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="name"/>
            <h:selectBooleanCheckbox id="nameValid" value="#{publicationBean.searchNameValid}"
                                     onclick="setEnabled('search:name',this.checked)"/>
            <h:inputText id="name" value="#{publicationBean.searchName}" size="70"/>
        </h:panelGroup>
        <script>
            setEnabled('search:name', ${publicationBean.searchNameValid})
        </script>


    </td>
</tr>

    <%-- Field: magazineDeliveryDate --%>
<tr>
    <td class="searchLabel">${msg['publication.magazineDeliveryDate']}:</td>
    <td class="searchFieldCell">


        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startMagazineDeliveryDate"/>
            <h:selectBooleanCheckbox id="startMagazineDeliveryDateValid"
                                     value="#{publicationBean.searchStartMagazineDeliveryDateValid}"
                                     onclick="setEnabled('search:startMagazineDeliveryDate',this.checked)"/>
            <t:inputCalendar id="startMagazineDeliveryDate" value="#{publicationBean.searchStartMagazineDeliveryDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true"
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:startMagazineDeliveryDate',true);setChecked('search:startMagazineDeliveryDateValid',true);"
                    />
        </h:panelGroup>
        -
        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endMagazineDeliveryDate"/>
            <h:selectBooleanCheckbox id="endMagazineDeliveryDateValid"
                                     value="#{publicationBean.searchEndMagazineDeliveryDateValid}"
                                     onclick="setEnabled('search:endMagazineDeliveryDate',this.checked)"/>
            <t:inputCalendar id="endMagazineDeliveryDate" value="#{publicationBean.searchEndMagazineDeliveryDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true"
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:endMagazineDeliveryDate',true);setChecked('search:endMagazineDeliveryDateValid',true);"
                    />
        </h:panelGroup>
        <script>
            setEnabled('search:startMagazineDeliveryDate', ${publicationBean.searchStartMagazineDeliveryDateValid})
            setEnabled('search:endMagazineDeliveryDate', ${publicationBean.searchEndMagazineDeliveryDateValid})
        </script>


    </td>
</tr>

    <%-- Field: magazinePublicationDate --%>
<tr>
    <td class="searchLabel">${msg['publication.magazinePublicationDate']}:</td>
    <td class="searchFieldCell">


        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startMagazinePublicationDate"/>
            <h:selectBooleanCheckbox id="startMagazinePublicationDateValid"
                                     value="#{publicationBean.searchStartMagazinePublicationDateValid}"
                                     onclick="setEnabled('search:startMagazinePublicationDate',this.checked)"/>
            <t:inputCalendar id="startMagazinePublicationDate"
                             value="#{publicationBean.searchStartMagazinePublicationDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true"
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:startMagazinePublicationDate',true);setChecked('search:startMagazinePublicationDateValid',true);"
                    />
        </h:panelGroup>
        -
        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endMagazinePublicationDate"/>
            <h:selectBooleanCheckbox id="endMagazinePublicationDateValid"
                                     value="#{publicationBean.searchEndMagazinePublicationDateValid}"
                                     onclick="setEnabled('search:endMagazinePublicationDate',this.checked)"/>
            <t:inputCalendar id="endMagazinePublicationDate" value="#{publicationBean.searchEndMagazinePublicationDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true"
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:endMagazinePublicationDate',true);setChecked('search:endMagazinePublicationDateValid',true);"
                    />
        </h:panelGroup>
        <script>
            setEnabled('search:startMagazinePublicationDate', ${publicationBean.searchStartMagazinePublicationDateValid})
            setEnabled('search:endMagazinePublicationDate', ${publicationBean.searchEndMagazinePublicationDateValid})
        </script>


    </td>
</tr>

    <%-- Field: ownPublicationDate --%>
<tr>
    <td class="searchLabel">${msg['publication.ownPublicationDate']}:</td>
    <td class="searchFieldCell">


        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="startOwnPublicationDate"/>
            <h:selectBooleanCheckbox id="startOwnPublicationDateValid"
                                     value="#{publicationBean.searchStartOwnPublicationDateValid}"
                                     onclick="setEnabled('search:startOwnPublicationDate',this.checked)"/>
            <t:inputCalendar id="startOwnPublicationDate" value="#{publicationBean.searchStartOwnPublicationDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true"
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:startOwnPublicationDate',true);setChecked('search:startOwnPublicationDateValid',true);"
                    />
        </h:panelGroup>
        -
        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="endOwnPublicationDate"/>
            <h:selectBooleanCheckbox id="endOwnPublicationDateValid"
                                     value="#{publicationBean.searchEndOwnPublicationDateValid}"
                                     onclick="setEnabled('search:endOwnPublicationDate',this.checked)"/>
            <t:inputCalendar id="endOwnPublicationDate" value="#{publicationBean.searchEndOwnPublicationDate}"
                             renderAsPopup="true" popupDateFormat="d/MM/yyyy" renderPopupButtonAsImage="true"
                             popupTodayString="#{msg['calendar.today']}" popupWeekString="#{msg['calendar.week']}"
                             onchange="setEnabled('search:endOwnPublicationDate',true);setChecked('search:endOwnPublicationDateValid',true);"
                    />
        </h:panelGroup>
        <script>
            setEnabled('search:startOwnPublicationDate', ${publicationBean.searchStartOwnPublicationDateValid})
            setEnabled('search:endOwnPublicationDate', ${publicationBean.searchEndOwnPublicationDateValid})
        </script>


    </td>
</tr>

    <%-- Field: accepted --%>
<tr>
    <td class="searchLabel">${msg['publication.accepted']}:</td>
    <td class="searchFieldCell">


        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="accepted"/>
            <h:selectBooleanCheckbox id="acceptedValid" value="#{publicationBean.searchAcceptedValid}"
                                     onclick="setEnabledByName('search:accepted',this.checked)"/>

            <h:outputLabel for="accepted" value="#{msg['msg.yesno']}"></h:outputLabel>
            <h:selectBooleanCheckbox id="accepted" value="#{publicationBean.searchAccepted}"/>


        </h:panelGroup>
        <script>
            setEnabledByName('search:accepted', ${publicationBean.searchAcceptedValid})
        </script>


    </td>
</tr>

    <%-- Ignored field: ownerId --%>


    <%-- Ignored field: departmentId --%>


    <%-- Ignored field: insertDate --%>


    <%-- Ignored field: updateDate --%>


    <%-- Field: magazine --%>
<tr>
    <td class="searchLabel">${msg['publication.magazine']}:</td>
    <td class="searchFieldCell">


        <h:panelGroup>
            <h:message styleClass="error" showSummary="true" showDetail="false" for="magazine"/>
            <h:selectBooleanCheckbox id="magazineValid" value="#{publicationBean.searchMagazineValid}"
                                     onclick="setEnabled('search:magazine',this.checked)"/>
            <h:selectOneMenu id="magazine" value="#{publicationBean.searchMagazine}"
                             onclick="setEnabled('search:magazine',true)">
                <f:selectItems value="#{publicationBean.magazines}"/>
                <f:converter converterId="autentia.EntityConverter"/>
            </h:selectOneMenu>
        </h:panelGroup>
        <script>
            setEnabled('search:magazine', ${publicationBean.searchMagazineValid})
        </script>


    </td>
</tr>

</table>

</h:form>
</f:view>

</body>
</html>  		
